par(mfrow=c(1,2))
acf(berk_diff, 20, xlim=c(1,20), ylim=c(-1,1), main="Differenced SACF")
pacf(stbarb_diff, 20, ylim=c(-1,1), main="Differenced PACF")
par(mfrow=c(1,2))
acf(berk_diff, 20, xlim=c(1,20), ylim=c(-.5,.5), main="Differenced SACF")
pacf(stbarb_diff, 20, ylim=c(-.5,.5), main="Differenced PACF")
par(mfrow=c(1,2))
acf(berk_diff, 20, xlim=c(1,20), ylim=c(-.5,.5), main="Berkeley Differenced SACF")
pacf(berk_diff, 20, ylim=c(-.5,.5), main="Berkeley Differenced PACF")
par(mfrow=c(1,2))
acf(stbarb_diff, 20, xlim=c(1,20), ylim=c(-.5,.5), main="Santa Barbara Differenced SACF")
pacf(stbarb_diff, 20, ylim=c(-.5,.5), main="Santa Barbara Differenced PACF")
